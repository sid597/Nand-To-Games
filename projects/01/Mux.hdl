/** 
 * Multiplexor:
 * out = a if sel == 0
 *       b otherwise
 */

CHIP Mux {
    IN a, b, sel;
    OUT out;

    PARTS:
    Not (in=sel,out=nots);
    And (a=a, b=nots, out=aAndNots);
    And (a=b, b=sel, out=bAndsel);
    Or (a=aAndNots, b=bAndsel, out=out);


}

  